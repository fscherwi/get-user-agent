  "dependencies": {
    "get-osx-version": "^3.0.0"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "ava": "^3.5.0",
    "coveralls": "^3.0.9",
    "eslint-plugin-jsdoc": "^22.0.0",
    "nyc": "^15.0.0",
    "ua-parser-js": "^0.7.21",
